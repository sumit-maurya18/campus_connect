{
  "name": "opportunity-portal-backend",
  "version": "1.0.0",
  "description": "Backend API for Opportunity Portal - Internships, Jobs, Hackathons, Scholarships",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage --verbose --forceExit",
    "test:watch": "jest --watch --forceExit",
    "test:unit": "jest tests/simple.test.js --forceExit",
    "test:integration": "jest tests/integration --forceExit",
    "test:quick": "jest tests/simple.test.js tests/integration/api.test.js --forceExit",
    "seed": "node src/utils/seeder.js -i",
    "seed:destroy": "node src/utils/seeder.js -d"
  },
  "keywords": [
    "opportunities",
    "internships",
    "jobs",
    "hackathons",
    "scholarships",
    "api",
    "express",
    "mongodb"
  ],
  "author": "Opportunity-portal team",
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jq": "^1.7.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
